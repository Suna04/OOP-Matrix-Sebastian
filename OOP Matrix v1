#ifndef MATRIX__KOORDINATE_H
#define MATRIX__KOORDINATE_H

using namespace std;
namespace Koordinate{

class Koordinate {
    int x;
    int y;

public:
    Koordinate();
    void setX(int x);
    void setY(int y);
    void ausgabe();
};
}
#endif //MATRIX__KOORDINATE_H

